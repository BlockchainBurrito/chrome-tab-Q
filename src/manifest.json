{
  "manifest_version": 3,
  "name": "Tabs limiter with queue",
  "short_name": "Tabs Queue",
  "description": "Tabs limiter with queue.",
  "version": "2.0.6",
  "permissions": [
    "tabs",
    "contextMenus",
    "storage"
  ],
  "icons": {
    "128": "images/default-128.png",
    "48": "images/default-48.png",
    "24": "images/default-24.png"
  },
"background": {
  "service_worker": "background.js"
},
  "options_page": "options.html",
  "action": {
    "default_icon": {
      "38": "images/default-32.png",
      "19": "images/default-20.png"
    },
    "default_popup": "popup.html"
  }
}